-- @atlcompiler emftvm
-- @nsURI UML=http://www.eclipse.org/uml2/5.0.0/UML
module Example;
create OUT : UML refining IN : UML;

rule Annotate {
	from
		s : UML!Classifier
	to
		t : UML!Classifier (
			ownedComment <- s.ownedComment->including(c)
		),
		c : UML!Comment (
			body <- 'Example comment for ' + s.name,
			annotatedElement <- t
		)
}
